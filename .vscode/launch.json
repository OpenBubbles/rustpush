    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rustpush-test'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rustpush-test",
                    "--package=rustpush"
                ],
                "filter": {
                    "name": "rustpush-test",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_LOG": "debug"
            }
        }
    ]
